					<option value="wide">fullwidth</option>
				</field>
				<field class="btn-group" name="header" type="radio" default="normal" label="Used Content normal or wide1366" description="">
					<option value="normal">normal</option>
					<option value="wide">wider</option>
				</field>
				<field name="boxedimage" type="t3media" default=""
				 label="Boxed image Background"
				 description=""/>

				<field name="politeloading" type="radio" default="0"
				 global="1"
				 label="Used Polite Loading"
				 description="Enabled or disabled animation polite loading">
					<option value="0">JNO</option>
					<option value="1">JYES</option>
				</field>
				<field name="lblsale" type="radio" default="0"
				 global="1"
				 label="Used label SALE"
				 description="Enabled or disabled label SALE ">
					<option value="0">JNO</option>
					<option value="1">JYES</option>
				</field>
				
				<field name="lblhot" type="radio" default="0"
				 global="1"
				 label="Used label HOT"
				 description="Enabled or disabled label HOT ">
					<option value="0">JNO</option>
					<option value="1">JYES</option>
				</field>
				<field name="lblhotcount" type="text" default="10"
				 global="1"
				 label="Used count label HOT"
				 description="Count label HOT ">
				</field>
				
				<field name="lbloffer" type="radio" default="0"
				 global="1"
				 label="Used label OFFER"
				 description="Enabled or disabled label OFFER">
					<option value="0">JNO</option>
					<option value="1">JYES</option>
				</field>
				
				<field name="lblsold" type="radio" default="0"
				 global="1"
				 label="Used label SOLD"
				 description="Enabled or disabled label SOLD">
					<option value="0">JNO</option>
					<option value="1">JYES</option>
				</field>
				
				<field name="lblnew" type="radio" default="0"
