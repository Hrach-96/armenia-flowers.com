					<option value="0">JNO</option>
					<option value="1">JYES</option>
				</field>
				<field name="showdesccount" type="text" default="50"
				 global="1"
				 label="Used count description"
				 description="Count description">
				</field>

				<field name="showcompare" type="radio" default="0"
				 global="1"
				 label="Used Show Compare on product"
				 description="Enabled or disabled compare">
					<option value="0">JNO</option>
					<option value="1">JYES</option>
				</field>
				<field name="showwishlist" type="radio" default="0"
				 global="1"
				 label="Used Show Wishlist on product"
				 description="Enabled or disabled wishlist">
					<option value="0">JNO</option>
					<option value="1">JYES</option>
				</field>
				<field name="showquick" type="radio" default="0"
				 global="1"
				 label="Used Show Quick on product"
				 description="Enabled or disabled Quick">
					<option value="0">JNO</option>
					<option value="1">JYES</option>
				</field>

