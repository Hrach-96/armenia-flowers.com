					<option value="Playfair Display">Playfair Display</option>

				</field>
				<field name="preloader" type="radio" default="0"
				 global="1"
				 label="Used preloader pages"
				 description="Enabled or disabled preloader pages ">
					<option value="0">JNO</option>
					<option value="1">JYES</option>
				</field>
				<field name="preloaderbg" type="text" default="#000000"
				 global="1"
				 label="Used background in preloader"
				 description="background default #000000">
				</field>
				<field name="stickynavigation" type="radio" default="0"
				 global="1"
				 label="Used sticky navigation"
				 description="Enabled or disabled sticky navigation ">
					<option value="0">JNO</option>
					<option value="1">JYES</option>
				</field>
				<field class="btn-group" name="layout" type="radio" default="boxed" label="Used layout boxed or wide" description="">
