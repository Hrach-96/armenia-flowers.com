				<fieldset name="general_params" label="T3_GENERAL_LABEL" description="T3_GENERAL_DESC">
				<field name="GoogleFonts" type="list" default="Noto Serif"
				 label="Used GoogleFonts List for Body"
				 description="">
					<option value="Noto Serif">Noto Serif</option>
					<option value="Roboto">Roboto</option>
					<option value="Lato">Lato</option>
					<option value="Ubuntu">Ubuntu</option>
					<option value="Open Sans">Open Sans</option>
					<option value="Source Sans Pro">Source Sans Pro</option>
					<option value="Raleway">Raleway</option>
					<option value="Roboto Slab">Roboto Slab</option>
					<option value="Droid Serif">Droid Serif</option>
					<option value="Merriweather">Merriweather</option>
					<option value="Merriweather Sans">Merriweather Sans</option>
					<option value="Playfair Display">Playfair Display</option>

				</field>
				<field name="GoogleFontsH" type="list" default="Noto Serif"
				 label="Used GoogleFonts List for Headings"
				 description="">
					<option value="Noto Serif">Noto Serif</option>
					<option value="Roboto">Roboto</option>
					<option value="Lato">Lato</option>
					<option value="Ubuntu">Ubuntu</option>
